  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2019",
    "noEmit": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": false
